#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#define MAX 10000


int main(){

	int n, k,atual = 0; //0 colombo, 1 carolina

	while(scanf("%d %d",&n,&k) != EOF){
		atual = 0;
		if((n%(k+1)) == 0)
			atual = 1;

		if(atual == 1)
			printf("Carolina\n");
		else
			printf("Colombo\n");			
	
	}

	return 0;
}
